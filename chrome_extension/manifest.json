{
  "manifest_version": 3,
  "name": "PhishGuard",
  "version": "1.0",
  "description": "Detects and alerts phishing URLs.",
  "permissions": ["tabs", "activeTab", "storage", "scripting"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "background": {
    "service_worker": "background.js"
  }
}
